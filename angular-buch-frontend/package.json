    "engines": {
        "node": "^22.0.0",
        "npm": "^10.0.0"
    },
    "scripts": {
        "ng": "ng",
        "start": "ng serve --host \"0.0.0.0\" --port 4200",
        "build": "ng build",
        "test:headless": "ng test --no-watch --browsers=ChromeHeadless --code-coverage",
